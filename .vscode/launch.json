            "args": [
                "ddd",
                "--exp_id=validation",
                "--dataset=wibam",
                "--no_color_aug",
                "--batch_size=4",
                "--num_epochs=70",
                "--lr_step=45,60",
                "--save_point=1,45,60,65",
                "--val_intervals=1",
                "--gpus=0",
                "--show_repro=True",
                "--load_model=exp/ddd/no_det_repro/model_1.pth",
